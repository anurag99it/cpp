#include <iostream>
#include<bitset>
using namespace std;
#define min(x,y)((x<y)?x:y)

int bitdifference(int a,int b)
{
   bitset <32> a_binary(a);
   bitset <32> b_binary(b);
   int sum=0;
     for(int i=31;i>=0;--i)
       if(a_binary[i]!=b_binary[i])
         ++sum;
    return sum;         
}
int main() {
	// your code go
  int n,s=0;
  cin>>n;
  int a[n];
   for(int i=0;i<n;i++)
     cin>>a[i];
    for(int i=0;i<n;i++)
     for(int j=0;j<n;j++)
        s+=bitdifference(a[i],a[j]);
     cout<<"  "<<s; 
     return 0;
}
